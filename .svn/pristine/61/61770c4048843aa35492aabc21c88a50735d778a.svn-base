/**app.wxss**/ 
.page {
  background-color: white;
  height: 100%;
  width: 750rpx;
  /* border-top: 1px solid #eeeff3; */
}
page{
  background-color:#eeeff3;
}
/*遮罩  */
.mask
{
  display:none;
  position:fixed;
  left:0;
  top:0;
  right:0;
  bottom:0;
  width:100%;
  height:100%;
  background:rgba(0,0,0,.2);
  z-index:11;
  transition: all 1s ease;
}
.mask.active
{
  display:flex;
}

.colum_contact
{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0);
}
/*去掉按钮边框*/
button[class="colum_contact"]::after {
  border: 0;
} 


