.search_type
{
  position:fixed;
  z-index:99;
  width:100%;
  height: 44px;
  background-color: white;
  top: 0px;
  align-items: center;
  display: flex;
  flex-direction: row;
  border-top: 1px solid #eeeff3;
}
.search_type_section
{
  width: 33.3%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 15px;
  border-bottom: 1px solid #eeeff3;
}
.search_type_section image
{
  width: 10px;
  height: 5px;
  
}
.type_normal
{
  color: rgb(27, 27, 27)
}
.type_select
{
  color: rgb(36, 196, 62);
}
.page__bd{
  margin-top: 44px;
  margin-right: 0;
  
}
.list
{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  background: #eceff2;
  box-sizing: border-box;
  
}  
.item
{
  width: 50%;
  background: white;
  border-radius: 5px; 
  display: flex;
  flex-direction: column;
  font-size:14px;
	box-sizing:border-box;
  border-bottom: 15px solid #eceff2;
  border-left: 5px solid #eceff2;
  border-right: 5px solid #eceff2;
  overflow: hidden;
  position: relative;
  
}
.photo
{
  width: 100%;
  margin: 0;
  height: 357rpx;
 
}
.play
{
  width: 46rpx;
  height: 46rpx;
  top: 290rpx;
  left: 20rpx;
  position: absolute;
  
}
.title
{
  margin-top: 8px;
  font-size: 15px;
  color: #000000;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
}
.detail
{
  margin-top: 5px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-right: 20px;
  margin-left: 5px;
  margin-bottom: 10px;
}
.colum
{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  text-align: center;
  height: 35px;
  font-size: 9px;
  color: #2a2a2a;
}
.colum1
{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.colum_heart
{
  margin-right: 20px;
}
.heart
{
  width: 17px;
  height: 15px;
}
.resale
{
  width: 16px;
  height: 16px;
}
.price
{
  color: #ff5656;
  font-size: 14px;
  
}
.old_price
{
  color: #5c5c5c;
  font-size: 11px;
  text-decoration: line-through;
  
}
.mongolia_left
{
  flex-direction: row;
  background: white;
  width: 750rpx;
  padding: 0;
  top: 44px;
}
.left_left
{
  width: 350rpx;
  height: 100%;
  background: #f6f6f9;
  padding: 0;
}
.left_left_item
{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eeeff3;
  font-size: 15px;
  height: 49px;
  line-height: 49px;
}
.left_left_item text
{
  margin-left: 15px;
}
.left_left_item image
{
  width: 9px;
  height: 17px;
  margin-right: 10px;
}

.left_right
{
  width: 400rpx;
  height: 100%;
}
.left_normal
{
  background: #f6f6f9;
  color: rgb(51, 51, 51);  
}
.left_select
{
  background: white;
  color: rgb(36, 196, 62);
}
::-webkit-scrollbar
{
  width: 0;
  height: 0;
  color: transparent;
}
.right_normal
{
  color: rgb(51, 51, 51);  
}
.right_select
{
  color: rgb(36, 196, 62);
}
.left_right_item
{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eeeff3;
  font-size: 15px;
  height: 49px;
  line-height: 49px;
}
.left_right_item text
{
  margin-left: 15px;
}
.left_right_item image
{
  width: 10px;
  height: 10px;
  margin-right: 10px;
}

.mongolia_center
{
  flex-direction: column;
  width: 750rpx;
  padding: 0;
  top: 44px;
}
.center_content
{
  background: white;
  width: 100%;
}
.center_item
{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 44px;
  font-size: 13px;
  border-bottom: 1px solid #eeeff3;
}
.center_item text
{
  margin-left: 15px;
}
.center_item image
{
  width: 10px;
  height: 10px;
  margin-right: 10px;
}
.center_normal
{
  color: rgb(102, 102, 102);
}
.center_select
{
  color: rgb(36, 196, 62);
}
.mongolia_right
{
 flex-direction: column;
  width: 750rpx;
  padding: 0;
  top: 44px;
}
.right_content
{
  display: flex;
  flex-direction: column;
  background: white;
  padding-left: 20rpx;
}
.right_title
{
  font-size: 15px;
  margin: 10px 0;
}
.right_section
{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.right_item
{
  width: 182.5rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 10px;
}
.right_item text
{
  width: 162.5rpx;
  height: 30px;
  line-height: 30px;
  border-radius: 5px;
  text-align: center;
  font-size: 13px;
}
.right_normal
{
  background: white;
  color: rgb(153, 153, 153);
  border: 1px solid #eeeff3;
} 
.right_select
{
  background: rgb(36, 196, 62);
  color: white;
  border: 0;
  
} 
.right_footer
{
  display: flex;
  flex-direction: row;
  margin-top: 10px;
  margin-bottom: 10px;
}
.right_footer text
{
  width: 345rpx;
  height: 40px;
  line-height: 40px;
  font-size: 15px;
  border-radius: 5px;
  text-align: center;
}
.right_footer_left
{
  border: 1px solid rgb(36, 196, 62);
  color: rgb(36, 196, 62);
  margin-right: 20rpx;
}
.right_footer_right
{
  color: white;
  background: rgb(36, 196, 62);

}


